                            <select id="selBlngMode" class="form-control">
                                <option>Cash</option>
                                <option>Cheque</option>
                                <option>Credit Card</option>
                                <option>RTGS</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-md-8">
