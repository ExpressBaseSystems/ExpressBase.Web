@{
    Layout = "~/Views/Shared/_Layout1.cshtml";
}

@await Component.InvokeAsync("PageHeaderCommon")

<link rel="stylesheet" href="~/css/EbOnBoarding/EbOnBoarding.css" />
<script src="~/js/EbOnBoarding/TenantDashBoard.js" type="text/javascript"></script>
@{
    var sso = ViewBag.IsSSO == "true" ? "true" : "false";
    var aptype = ViewBag.AppType != null ? ViewBag.AppType : 0;
}
<div class="row s-dash-container">
    <div class="pd-0 tdasboard-sidebar">
        <div class="t-container">
            <ul class="list-group menuwrapper_ul_outer">
                <li class="list-group-item outer_li">Solutions<i class="fa fa-chevron-right tdash-icon pull-right"></i></li>
                <li class="list-group-item outer_li">Billing<i class="fa fa-chevron-right tdash-icon pull-right"></i></li>
                <li class="list-group-item outer_li">Support<i class="fa fa-chevron-right tdash-icon pull-right"></i></li>
            </ul>
        </div>
    </div>
    <div class="s-dash-workplace">
        <div class="col-md-12 pd-0 s-dash-solution-desc" style="margin-bottom: 7px;">        
            <div class="col-md-10 pd-0">
                <h3 class="mr-t-0">Solutions <span class="apps_count"></span></h3>
            </div>
            <div class="col-md-2 pd-0">
                <a href='@Url.Action("CreateSolution","Tenant")' class="btn btn-info pull-right new_sol_lk"><i class="fa fa-plus"></i> New Solution</a>
            </div>
        </div>
        <div class="col-md-12 pd-0 s-dash-workplace-tabs">            
            <div class="tdash-box-body">   
                
            </div>
        </div>
    </div>
</div>
<div class="modal fade confTo-solution" id="confTo-solution" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
    <div class="modal-dialog">
        <div class="confTo-solution-container">
            <div class="confTo-solution-inner">
                <div class="heading">
                    Your Solution is ready now.
                </div>
                <div class="btn-section form-inline">
                    <button id="goToSolution" class="btn">Go to Application</button>
                    <button id="skip" class="btn pull-right">Continue.</button>
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    var Tdb = TenantDashBoard(@Html.Raw(ViewBag.Solutions), @sso, "@ViewBag.Email", @aptype);
    if ('@ViewBag.Msg')
        EbMessage("show", { Message: "@ViewBag.Msg", AutoHide: true });

</script>







