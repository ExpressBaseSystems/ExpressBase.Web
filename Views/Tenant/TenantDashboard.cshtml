            <ul class="list-group">
                <li class="list-group-item"><i class="fa fa-cog fa-lg tdash-icon" aria-hidden="true"></i> Solutions<div class="tr-on-hov"></div></li>
                <li class="list-group-item"><i class="fa fa-usd fa-lg tdash-icon" aria-hidden="true"></i> Billing<div class="tr-on-hov"></div></li>
                <li class="list-group-item"><i class="fa fa-life-ring fa-lg tdash-icon" aria-hidden="true"></i> Support<div class="tr-on-hov"></div></li>

            </ul>
        </div>
    </div>
    <div class="col-md-10 col-lg-10 pull-right t-dash-workplace">
        <div class="t-container">
            <div class="tdash-box">
                <div class="tdash-box-head">
                    Solutions
                </div>
                <div class="tdash-box-body">
                    @*<div class="tdash-sol-box">
                        <div class="solutionhead">
                            <h4>Al Arz</h4>
