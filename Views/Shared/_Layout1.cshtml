<body>
    <div id="tenatDtls" class="container-fluid">

        @{
            if (ViewBag.wc == "tc")
            {
                <div class="row">
                    @await Component.InvokeAsync("PageHeaderTenant", new { solnid = ViewBag.cid, email = ViewBag.email, console = ViewBag.wc });
                </div>
            }
            else
            {
                 <div id="submen" class="togglemenu"><i class="fa fa-bars fa-lg" alt="Brand"></i></div>

            }
        }
       
        <div class="row">
            @{
                if (ViewBag.wc == "dc" || ViewBag.wc == "uc")
                {
                    <div class="modal fade" tabindex="-1" role="dialog" id='ObjModal'>
                        <div class="modal-dialog modal-lg" role="document" style="width:90%">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <div id="headerDiv" style="height:50px;width:100%">
                                        @{
                                            if (ViewBag.wc == "dc" || ViewBag.wc == "uc")
                                            {
                                                Html.RenderPartial("PageHeaderDev");

                                            }
                                        }
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">&times;</span>
                                        </button>
                                    </div>
                                </div>
                                <div class="modal-body" style="height:500px;padding: 5px;">
                                    <div id="EbsideBar" class="col-md-3" style="height:100%;">
                                    </div>
                                    <div class="col-md-9" style="height:100%;">
                                        @{
                                            var hei = "93%";
                                            if (ViewBag.wc == "dc")
                                            {
                                                <div class="Eb-CommonMenuStyle">
                                                    <div id="topmenu" class="pull-right">
                                                        <button class="btn"><i class="fa fa-download" aria-hidden="true"></i></button>
                                                        <button class="btn"><i class="fa fa-upload" aria-hidden="true"></i></button>
                                                        <a href="" class="btn"><i class="fa fa-plus" aria-hidden="true"></i></a>
                                                    </div>
                                                </div>
                                                hei = "86%";
                                            }
                                            <div id="objList" style="height:@hei; overflow-y:auto; width: 100%;"></div>
                                        }
                                        <div class="input-group">
                                            <div class="input-group-btn">
                                                <button type="button" id="dropbuttn" class="btn btn-secondary dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                    All<span class="caret"></span>
                                                </button>
                                                <ul class="dropdown-menu" id="MyDropDownId">
                                                    <li><a href="#">All</a></li>
                                                </ul>
                                            </div>
                                            <input type="text" class="form-control" id="searchobj">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                }
                else
                {
                    <div id="EbsideBar" class="col-md-3" style="height:100%;display:none;">
                        @{ 

                            Html.RenderPartial("_SidebarMenu");
                        }

                    </div>

                }
            }

            <div id="layout_div" class="col-md-12 col-sm-12 col-xs-12 main-cont">
                @RenderBody()
                @RenderSection("EbHead", false)
            </div>

            @*<div class="col-md-12 col-sm-12 col-xs-12" style="padding:0;">
                    <div class="item">
                        <a href="#"> Home/</a>
                    </div>
                </div>*@
        </div>
    </div>
</body>
