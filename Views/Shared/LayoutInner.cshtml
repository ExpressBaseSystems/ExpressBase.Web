    <script>
        reload_if_session_expired();
        AllMetasRoot = {};
        var _uo = @Html.Raw(@ViewBag.UserObject);
        window.ebcontext = {
            sid: "@ViewBag.cid",
            user: _uo || null,
            menu: {},
            locations: {},
            header: {},
