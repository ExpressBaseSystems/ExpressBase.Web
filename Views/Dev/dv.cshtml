<div class="well well-sm">
    <div class='dropdown' id='datatSourceDropdown' style="display:inline-block">
        <button class='btn btn-primary dropdown-toggle' type='button' data-toggle='dropdown'>
            Select Datasource
            <span class='caret'></span>
        </button>
        <ul class='dropdown-menu scrollable-menu'>
            @{
                foreach (var element in ObjDSListAll)
                {
                    <li data-dsid="@element.Key">
                        <a href="#">
                            @element.Value.Name
                        </a>
                    </li>

                }
            }
        </ul>
    </div>
    <input id='dvnametxt' type='text' class='form-control' placeholder='dvname' style="display:none;width: auto;"/>
    <button id='Save_btn' class='btn btn-primary' style="float: right; display:none;"> Save </button>

</div>

<div id="filterDia"></div>


@section EbHead {
    <script>
        $("#datatSourceDropdown .dropdown-menu li a").on("click", function (e) {
            dsid = $(e.target).parent().attr("data-dsid");
            $("#datatSourceDropdown .btn:first-child").text($(e.target).text());
            $("#datatSourceDropdown .btn:first-child").val($(e.target).text());
            $.post('../Dev/DataVisualisation', { dsid: dsid }, function (data) {
                $("#filterDia").html(data);
                //$.LoadingOverlay("show");
            });
        });
        $(document).ready(function () {
            @Html.Raw(head)
            $('[data-toggle="tooltip"]').tooltip();
        });
    </script>
}

