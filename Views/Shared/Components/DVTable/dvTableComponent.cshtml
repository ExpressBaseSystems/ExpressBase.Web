
@{
    @using ExpressBase.Common;
    var dvobj = EbSerializers.Json_Serialize(ViewBag.dsObj);
}

<style>
    .tools {
        border: none;
        background: none;
        color: black;
    }
</style>
<link rel="stylesheet" type="text/css" href="~/css/splitWindow.css">
<script src="~/js/splitWindow.js" type="text/javascript"></script>
<div class="dv-body-dev">
    <div id="filter_Display" class="filter-display"></div>
    <div class="dv" id="parent-div@(ViewBag.tabnum)">
    </div>
</div>
    <script>
    if (@ViewBag.tabnum === 0) {
        var commonO = $("#layout_div").data("commonObj");
    }

    commonO.ObjCollection["#vernav@(ViewBag.tabnum)"] = new EbDataTable('@ViewBag.Refid', '@ViewBag.VersionNumber', @ViewBag.ObjType, @Html.Raw(ViewBag.dsObj), '@ViewBag.Status', @ViewBag.tabnum, '@ViewBag.ServiceUrl', '@ViewBag.wc', @ViewBag.counter, null, null, null, '@ViewBag.url');

    </script>
