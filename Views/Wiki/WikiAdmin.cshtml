                            <p class="WikiAdminMenuBar" style-val="Publish" id=""> Published</p>
                            <p class="WikiAdminMenuBar" style-val="Unpublish">  UnPublished</p>
                            <p class="WikiAdminMenuBar" style-val="PublicView"> Ordering </p>

                        </div>
                    </div>*@
                <div class="row wiki_admin_border" style="margin-bottom:5px">

                </div>
                <div class="row">
                    <div class="col-sm-12 public style-4" id="public" style=" display:flex; flex-flow:row wrap;padding-right: 0px;">

                    </div>
                </div>
            </div>
        </div>
