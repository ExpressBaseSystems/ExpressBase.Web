<script>

    var data = @appdata;
    var array_values = new Array();
    for (var key in data) {
        array_values.push(data[key]);
    }
    var Applications = new Bloodhound({
        datumTokenizer: Bloodhound.tokenizers.obj.whitespace('name'),
        queryTokenizer: Bloodhound.tokenizers.whitespace,

        local: $.map(data, function (app, i) {
            return {
                name: app
            };
        })
    });
    Applications.initialize();

    $('.app-container input').tagsinput({
        typeaheadjs: [{
            minLength: 1,
            highlight: true,
        }, {
            minlength: 1,
            name: 'Applications',
            displayKey: 'name',
            valueKey: 'name',
            source: Applications.ttAdapter()
        }],
        freeInput: true
    });

    $('.app-container input').on('beforeItemAdd', function(event) {
        if ($.inArray(event.item, array_values)===-1)
        {
            alert("invalid appliocation");
            event.cancel = true ;
        }
        else
        {
            event.cancel = false;
        }
    });
    var Tagdata ="@ViewBag.AllTags".split(",");
    var Tags = new Bloodhound({
        datumTokenizer: Bloodhound.tokenizers.obj.whitespace('name'),
        queryTokenizer: Bloodhound.tokenizers.whitespace,

        local: $.map(Tagdata, function (tag, i) {
            return {
                name: tag
            };
        })
    });
    Tags.initialize();

    $('.tag-container input').tagsinput({
        typeaheadjs: [{
            minLength: 1,
            highlight: true,
        }, {
            minlength: 1,
            name: 'Tags',
            displayKey: 'name',
            valueKey: 'name',
            source: Tags.ttAdapter()
        }],
        freeInput: true
    });
    var commonO = $("#layout_div").data("commonObj");
    if ('@ViewBag.Refid'.length > 0)
        commonO.UpdateCreateVersionDD();

    $(document).ready(function(){
        $('#commit_outer').on('click', function () {
            $('#changelogPopup').show();
        });
        $('#close_popup').on('click', function () {
            $('#changelogPopup').css("display", "none");
        });
        if ('@ViewBag.Tags'.length > 0)
        {
            $('#tags').tagsinput('add', '@ViewBag.Tags');
        }
        if ('@ViewBag.AppId'.length > 0)
        {
            $('#apps').tagsinput('add', '@ViewBag.AppId');
        }
        @*$('select[name=Apps]').val(@ViewBag.AppId);
        $('.selectpicker').selectpicker('refresh');*@

        //$(window).scroll(function () {
        //    if ($('.dashbord').css("display") === "block") {
        //        $('.dashbord').hide();
        //        $('.codedit').css("margin-top", "0px");
        //        $('#minmize').css("transform", "none");
        //    }
        //    else if ($('.dashbord').css("display") === "none") {
        //        $('.codedit').css("margin-top", " 85px");
        //        $('#minmize').css("transform", "rotate(180deg)");
        //    }
        //});
    });

        $('#minmize').click(function () {
            $('.dashbord').animate({ height: 'toggle' }, minMaxDash.bind(this));
        });
        minMaxDash = function() {
            if ($('.dashbord').css("display") === "none") {
                $('.codedit').css("margin-top", "34px");
                $('#minmize').css("transform", "rotate(180deg)");
                $(".tab-cont-common").css("height", "calc(100vh - 34px)");
            }
            else if ($('.dashbord').css("display") === "block") {
                $('.codedit').css("margin-top", "178px");
                $('#minmize').css("transform", "none");
                $(".tab-cont-common").css("height","calc(100vh - 178px)");
            }
        }

</script>
