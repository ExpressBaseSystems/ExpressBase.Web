<style>
    .mt-89 {
        margin-top: 89px;
    }
    .h-inherit {
        height: inherit;
        background: white;
    }
    .pd-0 {
        padding: 0;
    }
    .outer-row-profsetup {
        height: calc(100vh - 89px);
    }
    .profImageSec {
        background: #6da49f;
        height: inherit;
        padding: 30px;
    }

    .profSetupSec {
        height: inherit;       
    }

    .prof-picDiv {
        width: 100%;
        height: 150px;
        padding: 20px;
    }

    .profDetailHead {
        color: white;
        font-size: 22px;
        padding-left: 20px;
    }

    .profImagePrevSec {
        height: inherit;
    }

    .previewsec-outer {
        width: 100%;
        height: inherit;
        background: #efefef;
        padding: 35px 10px 10px 20px;
    }

        .previewsec-outer .cropsection {
            width: 100%;
            height: 300px;
        }

    .submitsec .btn {
        float: right;
    }

    .Eb-CommonMenuStyle {
        height: 32px;
        border: none;
    }

        .Eb-CommonMenuStyle .btn {
            border: none;
        }

    .croppie-container .cr-slider-wrap {
        width: 100%;
        padding: 5px;
        background-color: black;
        margin: 0;
        text-align: left;
    }

    .cr-slider {
        max-width: 100%;
        padding-top: 0;
        padding-bottom: 3px;
        background: white;
    }

    .cr-slider-wrap .btn {
        display: inline;
        margin-left: 5px;
        color: #fff;
        background-color: transparent;
        border-color: none;
    }

    .cr-slider-wrap input[type=range] {
        width: 99px;
        display: inline;
    }

    .imgsec-text {
        margin-top: 30px;
        color: white;
    }
</style>
