    <script>


            $(document).scroll(function () {
                var y = $(this).scrollTop();
                if (y > $('.slider').height()) {
                    $('#trial-btn').fadeIn();
                } else {
                    $('#trial-btn').fadeOut();
                }

                $('#btnGroupAddon').click(function () {
                    document.getElementById("signup_form").submit();
                })
            });

            function validate(form) {
                var email = form.Email.value.trim();
                if (ValidateEmail(email))
                    return true;
                else
                    return false;
            }
            function ValidateEmail(mail) {
                if (/^\w+([\.-]?\w+)*@@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(mail))
                    return (true);
                return (false);
            }
            $(".joinbeta").submit(function (e) {
                e.preventDefault();
                if (validate(this)) {
                    $.ajax({
                        type: 'POST',
                        url: "../Ext/JoinBeta",
                        beforeSend: function () {
                            $(e.target).children().find('i').show();
                        },
                        data: $(e.target).serializeArray()
                    }).done(function (data) {
                        var msg = data ? "Thank you for your interest in our private beta. We will get back to you shortly." : "You have already specified your interest to join our private beta. We will get back to you shortly.";
                        $(e.target).children().find('i').hide();
                        $(e.target).children().find(".emailtxt").val("");
                        $(".message-box-indexpage").fadeIn().children("span").text(msg);
                        $(".ext-pageheader .navbar").css("top", "52px");
                        $(".slider").css("padding-top", "100px");
                        $("#close-msg").click(function (e) {
                            $(".message-box-indexpage").fadeOut();
                            $(".ext-pageheader .navbar").css("top", "0");
                            $(".slider").css("padding-top", "0");
                        });
                    });
                }
            });

            $(".icon-round, .icon-round i").on("click", function (e) {
                d = document;
                //$(d).scrollTop(1050);
                var $round = $(event.target).closest(".icon-round");
                var appid = $round.attr("appid");
                if (d.latestAppid === appid)
                    return;
                var roundIdx = $(".icon-round").toArray().indexOf($round[0]);
                d.appIdCount = roundIdx;
                $round.css({
                    "border-bottom": "4px solid green",
                    "background": "#e5f3f5",
                    "outline": "none"
                });
                $(".icon-round").siblings(".icon-label").css("border-bottom", "solid 1px #ccc").css("color", "#8A939E");

                $round.parent(".liveDemos_iconContainers").siblings().each(function (i, obj) {
                    $(obj).children(".icon-round").css({
                        "border-bottom": "none",
                        "background": "none",
                        "outline": "none"
                    });
                });
                var html = d.getElementsByTagName('html')[0];
                html.style.setProperty("--ebbotThemeColor", d.ebbotThemeColorColl[roundIdx]);

                if (appid === undefined)
                    return;
                if ($(`.usecase-bots-cont [appid=${appid}]`).length === 0)
                    d.createBotFns[roundIdx]();
                $(".eb_iframecont").not(`[appid=${appid}]`).animate({ opacity: '0' }, 0).css("right", "-5%").hide(300);
                $(`.usecase-bots-cont [appid=${appid}]`).show(0, function () { $(this).animate({ opacity: '1', right: '5%' }, 400); });

                $(".content").css("opacity", "0").hide(200);
                $(`.content:eq(${roundIdx})`).show(0, function () { $(this).animate({ opacity: '1' }, 400); });
                d.latestAppid = appid;
            });
    </script>
