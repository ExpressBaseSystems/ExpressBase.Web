<body>
    @{if (ViewBag.wc == "tc")
            {
                Html.RenderPartial("_PageheaderTenant");
            }
            else if (ViewBag.wc == "dc" || ViewBag.wc == "uc")
            {
                Html.RenderPartial("_PageheaderDev");

            }
    }
    <div id="tenatDtls" class="container-fluid">
        <div class="row">
            <div class="col-md-3 side">
                @if (ViewBag.wc == "tc")
                {
                    Html.RenderPartial("_SidebarMenu");
                }
                else if (ViewBag.wc == "dc")
                {
                    Html.RenderPartial("_SidebarDev");
                }
                else
                {
                    Html.RenderPartial("_SidebarmenuTUser");

                }

            </div>
            <div class="col-md-9 col-sm-12 col-xs-12 pull-right" style="padding:0;">
                <div class="item">
                    <a href="#"> Home/</a>
                </div>
            </div>
            <div id="layout_div" class="col-md-9 col-sm-12 col-xs-12 main-cont pull-right">
                <i class="fa fa-spinner fa-pulse fa-4x fa-fw center-block" id="loader" aria-hidden="true" style="display:none;margin:200px 400px;color:#808080;z-index:5;">  </i>
                @RenderBody()
                @RenderSection("EbHead", false)
            </div>
        </div>
    </div>
</body>
