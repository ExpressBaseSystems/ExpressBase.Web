            EbObjList: @Html.Raw(ViewBag.SideBarMenu),
            googlekey: '@ViewBag.googlekey',
            Toolhtml:`@Html.Raw(_toolBox.ToolsHtml)`,
        });

        commonO.ObjCollection["#vernav@(ViewBag.tabnum)"] = DBobj;


