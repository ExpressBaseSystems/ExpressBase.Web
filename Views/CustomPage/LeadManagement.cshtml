<!-- Surgery Details Modal -->
<div class="modal fade" id="mdlSurgery" role="dialog" data-id="">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Surgery Details</h4>
            </div>
            <div class="modal-body" style="height: 270px;">
                <div class="lm-form-inline">
                    <div class="col-md-4">
                        <div class="lm-form-group">
                            <label>Date</label>
                            <div class="input-group">
                                <input id="txtSrgyDate" type="text" class="form-control" autocomplete="off">
                                <span class="input-group-addon" onclick="$('#txtSrgyDate').focusin();"><i class="fa fa-calendar"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4"></div>
                    <div class="col-md-4">
                        <div class="lm-form-group">
                            <label>Branch</label>
                            <select id="selSrgyBranch" class="form-control">
                                <option>Kochi</option>
                                <option>Kozhikode</option>
                                <option>Trivandrum</option>
                                <option>Coimbatore</option>
                            </select>
                            @*<input id="txtSrgyBranch" type="text" class="form-control">*@
                        </div>
                    </div>
                </div>
                <div class="lm-form-inline">
                    <div class="col-md-4">
                        <div class="lm-form-group">
                            <label>Zone</label>
                            <input id="txtSrgyZone" type="text" class="form-control">
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="lm-form-group">
                            <label>Grafts</label>
                            <input id="txtSrgyGrafts" type="text" class="form-control">
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="lm-form-group">
                            <label>Density</label>
                            <input id="txtSrgyDensity" type="text" class="form-control">
                        </div>
                    </div>
                </div>
                <div class="lm-form-inline">
                    <div class="col-md-6">
                        <div class="lm-form-group">
                            <label>Patients Instructions</label>
                            <textarea id="txaPatientInstr" class="form-control" style="height: 100px !important; resize:none;"></textarea>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="lm-form-group">
                            <label>Doctors Instructions</label>
                            <textarea id="txaDoctorInstr" class="form-control" style="height: 100px !important; resize:none;"></textarea>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button id="btnSrgySave" class="btn btn-default" type="button"><i class="fa fa-spinner fa-pulse" aria-hidden="true" style="display:none;"></i> Save</button>
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

<style>
    .MyTempColStyle {
        /*white-space: nowrap;
        overflow: hidden;*/
        overflow: visible;
        white-space: normal !important;
        /*text-overflow: ellipsis;*/
        max-width: 200px;
        height: 30px !important;
    }
    /*.MyTempColStyle:hover {
            overflow: visible;
            white-space: normal;
        }*/
</style>




@{
    @using Newtonsoft.Json;
    var CInfo = Html.Raw(JsonConvert.SerializeObject(ViewBag.CustomerData));
    var CenterInfo = Html.Raw(JsonConvert.SerializeObject(ViewBag.CostCenter));
    var DocInfo = Html.Raw(JsonConvert.SerializeObject(ViewBag.DocDict));
    var StaffInfo = Html.Raw(JsonConvert.SerializeObject(ViewBag.StaffDict));

    var CCityList = Html.Raw(JsonConvert.SerializeObject(ViewBag.CrntCityList));
    var CCountryList = Html.Raw(JsonConvert.SerializeObject(ViewBag.CrntCountryList));
    var CityList = Html.Raw(JsonConvert.SerializeObject(ViewBag.CityList));
    var SourceCategoryList = Html.Raw(JsonConvert.SerializeObject(ViewBag.SourceCategoryList));
    var SubCategoryList = Html.Raw(JsonConvert.SerializeObject(ViewBag.SubCategoryList));

    var FeedbackList = (Html.Raw(ViewBag.FeedbackList).ToString() == "") ? "null" : Html.Raw(ViewBag.FeedbackList);
    var BillingList = (Html.Raw(ViewBag.BillingList).ToString() == "") ? "null" : Html.Raw(ViewBag.BillingList);
    var SurgeryList = (Html.Raw(ViewBag.SurgeryList).ToString() == "") ? "null" : Html.Raw(ViewBag.SurgeryList);
}

<script>
    var leadObject = new LeadManagementObj(@ViewBag.AccId, @ViewBag.MC_Mode, @CInfo, @CenterInfo, @DocInfo, @StaffInfo, @FeedbackList, @BillingList, @SurgeryList, @CCityList, @CCountryList, @CityList, @SourceCategoryList, @SubCategoryList );
</script>

