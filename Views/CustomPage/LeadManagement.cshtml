</div>


<!-- Add Followup Details Modal -->
<div class="modal fade" id="mdlFeedBack" role="dialog" data-id="">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Feedback Details</h4>
            </div>
            <div class="modal-body" style="height: 270px;">
                <div class="lm-form-inline">
                    <div class="col-md-6">
                        <div class="lm-form-group">
                            <label>Date</label>
                            <div class="input-group">
                                <input id="txtFlUpDate" type="text" class="form-control">
                                <span class="input-group-addon" onclick="$('#txtFlUpDate').focusin();"><i class="fa fa-calendar"></i></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="lm-form-inline">
                    <div class="col-md-6">
                        <div class="lm-form-group">
                            <label>Lead Status</label>
                            <input id="txtFlUpStatus" type="text" class="form-control">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="lm-form-group">
                            <label>Follow up Date</label>
                            <div class="input-group">
                                <input id="txtFlUpFolDate" type="text" class="form-control">
                                <span class="input-group-addon" onclick="$('#txtFlUpFolDate').focusin();"><i class="fa fa-calendar"></i></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="lm-form-inline">
                    <div class="col-md-12">
                        <div class="lm-form-group">
                            <label>Comment</label>
                            <textarea id="txaFlUpComnt" class="form-control" style="height: 100px !important; resize:none;"></textarea>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button id="btnFlUpSave" class="btn btn-default" type="button"><i class="fa fa-spinner fa-pulse" aria-hidden="true" style="display:none;"></i> Save</button>
            </div>
        </div>
    </div>
</div>

<!-- Graft Details Modal -->
<div class="modal fade" id="mdlGraft" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Graft Details</h4>
            </div>
            <div class="modal-body form-cont" style="height: 270px;">


                <div class="lm-form-inline">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>No of Grafts</label>
                            <input type="text" class="form-control">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>Total Rate</label>
                            <input type="text" class="form-control">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>No of PRP</label>
                            <input type="text" class="form-control">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>Consulted Date</label>
                            <input type="text" class="form-control">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>Fee Paid</label>
                            <input type="text" class="form-control">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>Consulting Doctor</label>
                            <input type="text" class="form-control">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>Probable Month</label>
                            <input type="text" class="form-control">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>Closing</label>
                            <input type="text" class="form-control">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>Nature of Patient</label>
                            <input type="text" class="form-control">
                        </div>
                    </div>
                </div>


            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button id="btnGraftSave" class="btn btn-default" type="button">Save</button>
            </div>
        </div>
    </div>
</div>


<style>
    .MyTempColStyle {
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;
        max-width: 200px;
    }
</style>




@{
    @using Newtonsoft.Json;
    var CInfo = Html.Raw(JsonConvert.SerializeObject(ViewBag.CustomerData));
    var CenterInfo = Html.Raw(JsonConvert.SerializeObject(ViewBag.CostCenter));

    var FeedbackList = (Html.Raw(ViewBag.FeedbackList).ToString() == "") ? "null" : Html.Raw(ViewBag.FeedbackList);

}

<script>
    var leadObject = new LeadManagementObj(@ViewBag.AccId, @ViewBag.MC_Mode, @CInfo, @CenterInfo, @FeedbackList);
</script>