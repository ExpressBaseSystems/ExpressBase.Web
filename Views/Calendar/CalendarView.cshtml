@{
    Layout = "~/Views/Shared/LayoutBuilders.cshtml";
    var bApnd = true;
    if (ViewBag.Env == "Development")
    {
        bApnd = false;
    }
}

<link rel="stylesheet" type="text/css" href="~/css/bootstrap-select.min.css" />
<link rel="stylesheet" type="text/css" href="~/css/bootstrap-multiselect.css" />
<link rel="stylesheet" type="text/css" href="~/css/CalendarStyles.css" asp-append-version=@bApnd />
<link rel="stylesheet" type="text/css" href="~/css/Eb_datatable.css" asp-append-version=@bApnd />
<link rel="stylesheet" type="text/css" href="~/css/splitWindow.css" asp-append-version=@bApnd />
<script type="text/javascript" src="~/js/jquery.inputmask.bundle.js"></script>
<script src="~/js/EbCalendar/EbCalendarView.js" asp-append-version=@bApnd></script>
<script src="~/js/CommonDataTable.js" asp-append-version=@bApnd></script>
<script src="~/js/EbDataTable.js" asp-append-version=@bApnd></script>
<script src="~/js/splitWindow.js" asp-append-version=@bApnd></script>
@*<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.12.2/js/bootstrap-select.min.js"></script>*@
<script type="text/javascript" src="~/js/LayoutCommon/bootstrap-select.min.js"></script>
<script type="text/javascript" src="~/js/bootstrap-multiselect.js"></script>

@await Component.InvokeAsync("PageHeaderCommon")

<div class="container-fluid calendar_wrapper" style="height: calc(100vh - 92px);margin-top: 38px;">
    <div class="row calendar-user-view-cont" style="height:inherit;">
        <div class="col-sm-12" id="calendar-user-view" style="height:inherit">
        </div>
    </div>

    <div id="ppt-calendar-view" class="PGgrid"> <div id="propGridView"> </div></div>
</div>
<script type="text/javascript">
     $('title').text('@ViewBag.DisplayName');
    @Html.Raw(ViewBag.Meta);
    @Html.Raw(ViewBag.JsObjects);
    @Html.Raw(ViewBag.EbObjectType);
    var DBobj = new EbCalendar({
         RefId: '@ViewBag.Refid',
        Version: '@ViewBag.VersionNumber',
        ObjType: @ViewBag.ObjType,
        dvObj: @Html.Raw(ViewBag.dsObj),
        Status: '@ViewBag.Status',
        ServiceUrl: '@ViewBag.ServiceUrl',
        Wc: '@ViewBag.wc',
        Cid: '@ViewBag.cid',
    });
</script>