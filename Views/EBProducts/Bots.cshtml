<script>
                        $(document).scroll(function () {
                            var y = $(this).scrollTop();
                            if (y > $('.slider').height()) {
                                $('#trial-btn').fadeIn();
                                $('.navbar-default').css({ "background-color": "var(--ebbg)", "background-image": "var(--ebbgimage)" });
                            } else {
                                $('#trial-btn').fadeOut();
                                $('.navbar-default').css({ "background-color": "transparent", "background-image": "none" });
                            }

                            $('#btnGroupAddon').click(function () {
                                document.getElementById("signup_form").submit();
                            })
                        });

                        $(".icon-round, .icon-round i").on("click", function () {
                            var $round = $(event.target).closest(".icon-round");
                            var appid = $round.attr("appid");
                            $(".icon-round").css("filter", "none").css("border-width", "1px").css("border-bottom", "none").css("border-right", "none");
                            $round.css("filter", "saturate(2)").css("border-width", "3px").css("border-bottom", "solid 1px white").css("border-right", "solid 1px white");
                            if (appid === undefined)
                                return;
                            var roundIdx = $(".icon-round").toArray().indexOf($round[0]);
                            if ($(`.usecase-bots-cont [appid=${appid}]`).length === 0)
                                document.createBotFns[roundIdx]();
                            $(".eb_iframecont").hide(300);
                            $(`.usecase-bots-cont [appid=${appid}]`).show(300);

                            $(".content").css("opacity", "0").hide(200);
                            $(`.content:eq(${roundIdx})`).show(0, function () { $(this).animate({ opacity: '1' }, 400); });



                            //$(".icon-round").css("border-color", "#8bb7f0");
                            //$round.css("border-color", "#60dc60");
                        });
</script>
