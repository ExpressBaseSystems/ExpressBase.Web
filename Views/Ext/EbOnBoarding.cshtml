<div class="row tAddAcc height-100">
    <div class="row_eblog_t"><img src="~/images/EB_Logo-png.png" /></div>
    <section class="card">
        <div class="card--content mr-l-20" id="prof-info">
            <div class="loader_top" id="loader_profile"></div>
            <div id="firstcrd" class="col-md-8 card--form">
                <div class="products-head">
                    <h4>Profile Setup</h4>
                </div>
                <div class="card--content-body">
                    <form class="form-horizontal" id="prof-submit" method="post">
                        <br />
                        <div class="col-md-12">
                            <div class="form-group">
                                <label>Email/Username</label>
                                <div class="input-group">
                                    <span class="input-group-addon" id="basic-addon1"><i class="fa fa-envelope-o" aria-hidden="true"></i></span>
                                    <input class="form-control" type="text" value="@ViewBag.useremail" style="background-color:#f5f5f5" name="Email" id="email" readonly>
                                </div>
                            </div>
                            <div class="eb_required_group required">
                                <label>Name</label>
                                <input class="form-control txthighlight" type="text" name="Name" id="name">
                                <p id="namelbl" style="color:red; visibility :hidden"> Enter a name</p>
                            </div>
                            <div class="eb_required_group required">
                                <label>Company</label>
                                <input class="form-control txthighlight" type="text" name="Company" id="company">
                                <p id="companylbl" style="color:red; visibility : hidden"> Enter a company</p>
                            </div>
                            <input type="hidden" name="Country" id="country" required>
                            <div class="eb_required_group required">
                                <div class="pd-l-0 col-md-6">
                                    <label>Password</label>
                                    <div class="input-group">
                                        <span class="input-group-addon" id="basic-addon1"><i class="fa fa-key" aria-hidden="true"></i></span>
                                        <input class="form-control txthighlight" data-minlength="8" type="password" name="Password" id="inputPassword">
                                        <span toggle="#inputPassword" style=" padding-right: 13px;" id="psvisible" class="fa fa-fw fa-eye fa-eye-slash field-icon toggle-password "></span>
                                        <span toggle="#inputPassword" style="padding-right: 13px;" hidden id="pshide" class="fa fa-fw fa-eye  field-icon toggle-password "></span>
                                    </div>

                                    @*<div class="inline-block">
                                            <i class="fa fa-info-circle" data-toggle="tooltip" style="font-size:15px;color:dodgerblue"></i>
                                            <span class="inline-block help-block">Minimum of 8 characters</span>
                                        </div>
                                        <span class="inline" style="margin-top:3px;display:inline-block" id="psdinfo">
                                            <span> <i id="psdinfo1" class="fa fa-info-circle " style="color:dodgerblue;display:inline-block"></i></span>&nbsp&nbsp&nbsp
                                            <span class="help-block inline" style="display:inline-block;"><p id="passlbl"> Minimum of 8 characters </p></span>
                                        </span>
                                    *@

                                    <div style="margin-top:1px;margin-bottom:2px; display:inline-block " id="psdinfo" class="inline_css">
                                        <span> <i id="psdinfo1" class="fa fa-info-circle" style="color:dodgerblue;display:inline-block"> </i>  </span>
                                        <span style="display:inline-block"> <p id="passlbl" style="color:cornflowerblue;margin-bottom:0px" class="p_hover">Enter a strong password</p></span>
                                    </div>
                                    <div id="rcorners1" style="position:absolute; visibility:hidden">
                                        <div style="margin-top=0px;margin-bottom:0px; display:inline-block ">
                                            <span> <i id="passcheck_1" class="fa fa-times" style="color:#cf4f4f;font-size:11px;display:inline-block"> </i>  </span>
                                            <span style="display:inline-block"> <p style="margin-bottom:0px" class="p_hover">At least eight characters.</p></span>
                                        </div>
                                        <div style="margin-top=0px;margin-bottom:0px; display:inline-block ">
                                            <span> <i id="passcheck_2" class="fa fa-times" style="color:#cf4f4f;font-size:11px;display:inline-block"> </i>  </span>
                                            <span style="display:inline-block"> <p style="margin-bottom:0px" class="p_hover">At least one uppercase character.</p></span>
                                        </div>
                                        <div style="margin-top=0px;margin-bottom:0px; display:inline-block ">
                                            <span> <i id="passcheck_3" class="fa fa-times" style="color:#cf4f4f;font-size:11px;display:inline-block"> </i>  </span>
                                            <span style="display:inline-block"> <p style="margin-bottom:0px" class="p_hover">At least one lowercase character.</p></span>
                                        </div>
                                        <div style="margin-top=0px;margin-bottom:0px; display:inline-block ">
                                            <span> <i id="passcheck_4" class="fa fa-times" style="color:#cf4f4f;font-size:11px;display:inline-block"> </i>  </span>
                                            <span style="display:inline-block"> <p style="margin-bottom:0px" class="p_hover">At least one number.</p></span>
                                        </div>
                                        <div style="margin-top=0px;margin-bottom:0px; display:inline-block ">
                                            <span> <i id="passcheck_5" class="fa fa-times" style="color:#cf4f4f;font-size:11px;display:inline-block"> </i>  </span>
                                            <span style="display:inline-block"> <p style="margin-bottom:0px" class="p_hover">At least one special character</p></span>
                                        </div>

                                    </div>

                                </div>
                                <div class="pd-r-0 col-md-6">
                                    <label>Repeat Password</label>
                                    <input class="form-control txthighlight" type="password" name="Repeat-password" id="inputPasswordConfirm" data-match="#inputPassword" data-match-error="Password donot match">
                                    <div class="help-block with-errors">
                                        <p id="repeat_passwordlbl" style="color:red;margin-bottom: 0px; visibility :hidden"> Password Didn't match</p>
                                    </div>
                                </div>
                                <input type="submit" id="prof-form-sub" style="display:none;" />
                            </div>
                        </div>
                    </form>
                </div>
                <div class="card--content-footer">
                    <div class="form-inline">
                        <a id="prof-info-skip" class="ebbtn pull-right ebbtn eb_btngreen eb_btn_md prof-info-skip">Next <span class="fa fa-chevron-right" aria-hidden="true"></span></a>
                        <button id="save-profile" onclick="$('#prof-form-sub').click()" type="submit" class="submit-validate ebbtn eb_btngreen eb_btn_md pull-right">Next <i class="fa fa-chevron-right" aria-hidden="true"></i></button>

                    </div>
                </div>
