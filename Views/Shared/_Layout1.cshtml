                    <div class="col-sm-2 nopadding nomargin">
                        <div class="affix">*@
                @{ if (ViewBag.Fname != null) { Html.RenderPartial("_SidebarMenu"); } }
                @*</div>*@
            </div>
            <div id="page-content-wrapper">
                <div class="container-fluid">
                    @*<div class="row">
                        <div class="col-lg-12">
                            <br>
                            <h1>Your Title</h1>
                        </div>
                    </div>*@
                    <div class="row">
                        <div class="col-lg-12">
                            @RenderBody()
                            @RenderSection("EbHead", false)
                        </div>
                    </div>
                    @*<div class="col-sm-10 nopadding nomargin">*@
                    @*@RenderBody()
                    @RenderSection("EbHead", false)*@
                    @*</div>*@
                </div>
            </div>
        </div>
    </div>
</body>
