<body>
    <div class="html-root">
        @{
            if (ViewBag.UId > 1 || ViewBag.cide == "demo")
            {
                if (ViewBag.wc == "dc" || ViewBag.wc == "uc" || ViewBag.wc == "tc")
                {
                    <div id="eb-common-menu">
                        @{ await Html.RenderPartialAsync("EbQuickMenu");}
                    </div>
                }
            }
            if (ViewBag.wc == "uc")
            {
                if (ViewBag.UId > 1 || ViewBag.cide == "demo")
                {
                    <div id="eb-location-switch">
                        @{ await Html.RenderPartialAsync("EbLocationSwitch");}
                    </div>
                    <div id='eb-finyear-switch'></div>
                }
            }
        }
        <div id="layout_div" class="container-fluid html-root-layout">
            @if (ViewBag.wc == "dc" || ViewBag.wc == "uc" || ViewBag.wc == "tc")
            {
                @await Component.InvokeAsync("PageHeaderCommon")
            }
            @RenderBody()
        </div>
        @if (ViewBag.UId > 1 || ViewBag.cide == "demo")// check for PublicForm. Can rewite this check if required.
        {
            <div class="eb-expndLinkswrpr hidden-xs" id="eb-expndLinkswrprWdgt">
                <button id="eb-expandbutton" onclick="$('.eb-expndLinkswrpr-body').toggle();" class="eb-expand-link"><i class="fa fa-cog"></i></button>
                <div class="eb-expndLinkswrpr-body" style="display:none;">
                    <div class="eb-expndLinkswrpr-inner">
                        <button class="eb-expand-innerbtn" onclick="if (window.tour) {window.tour.start();} $('.eb-expndLinkswrpr-body').toggle();" title="Walk through">
                            <i class="fa fa-plane"></i>
                        </button>
                        @*<button class="eb-expand-innerbtn" title="Shortcuts"><i class="fa fa-random"></i></button>*@
                    </div>
                </div>
            </div>
        }

        <div class="modal fade eb-common-popup" id="eb-common-popup" tabindex="-1" role="dialog" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title">Modal title</h5>
                        <button type="button" class="close" id="eb-common-popup-close" data-dismiss="modal" style="margin-left:auto;">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">

                    </div>
                    <div class="modal-footer">
                        <button type="button" id="eb-common-popup-ok" class="btn btn-primary">OK</button>
                    </div>
                </div>
            </div>
        </div>
        @if (ViewBag.wc == "uc")
        {
            await Html.RenderPartialAsync("NotificationWindow");
        }
    </div>

    @if (ViewBag.wc == "uc" || ViewBag.wc == "dc" || ViewBag.wc == "tc")
    {
        <script>
            window.ebcontext.menu = new EbMenu({
                Console: '@ViewBag.wc',
                Sid: '@ViewBag.cid',
                Uid: '@ViewBag.UId'
            });
        </script>
    }
    @if (ViewBag.wc == "uc")
    {
        <script>
            var loc__ = new LocationPicker({
                Location: '@Html.Raw(ViewBag.Locations)',
                Tid: '@ViewBag.cid',
                Uid:'@ViewBag.UId',
                Current:@ViewBag.CurrentLocation
            });

            window.ebcontext.locations = loc__;

            window.ebcontext.finyears = new FinYearPicker({
                FinYears: @Html.Raw(ViewBag.FinYears),
                Tid: '@ViewBag.cid',
                Uid:'@ViewBag.UId',
            });
    
        window.ebcontext.languages = new LanguagePicker({
            LanguagesList:@Html.Raw(ViewBag.Languages??"''"),
            Tid: '@ViewBag.cid',
            Uid: '@ViewBag.UId',
        });
        </script>
    }

    @if (IsSectionDefined("JsCode"))
    {
        @RenderSection("JsCode")
    }
    @Html.RenderJsCode()

