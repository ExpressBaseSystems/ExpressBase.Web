    <script>
        $('document').ready(function () {
            $('#submen').click(function () {
                $('.side').toggle();
                if ($('.side').css("display") === "none")
                    $('div[id^="layout_div"]').removeClass('col-md-9').addClass('col-md-12');
                else
                    $('div[id^="layout_div"]').removeClass('col-md-12').addClass('col-md-9');
            });

            $('.list-group a').on('click', function () {
                var $this = $(this),
                    $bc = $('<div class="item"></div>');

                $this.parents('li').each(function (i, parent) {
                    var i = $(parent).children('a').text();
                    $bc.prepend(' / ', i);
                    parent = i;
                });
                $('.item').html($bc.prepend('<a href="#"> Home</a>'));
            })           
        })
    </script>
