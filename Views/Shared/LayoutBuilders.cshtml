<body>
    @if (ViewBag.wc == "dc" || ViewBag.wc == "uc")
    {
        Html.RenderPartial("EbQuickMenu");
    }
    <div id="tenatDtls" class="container-fluid">

        @if (ViewBag.wc == "uc")
        {
            <div class="loc_switchModal_fade"></div>
            <div class="loc_switchModal" id="loc_switchModal">
                <div class="loc_switchModal_outer">
                    <div class="loc_switchModal_box">
                        <div class="locs_head">
                            <h4>Switch Location</h4>
                            <div class="loc_searchInput w-100">
                                <input type="text" class="form-control" id="loc-search" placeholder="Search Location." />
                            </div>
                        </div>
                        <div class="locs_bdy"></div>
                        <div class="locs_foot">
                            <button class="btn loc-switchbtn pull-right" id="setLocSub">Switch</button>
                        </div>
                        <div class="no_loc_config">No Locations.</div>
                    </div>
                </div>
            </div>
        }
    </div>
    <div id="layout_div" class="container-fluid main-cont">
        @RenderBody()
    </div>
</body>
