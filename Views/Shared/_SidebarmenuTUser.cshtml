<script>
    function LoadObjectListUser(type44) {       
        event.preventDefault();
        var _container = $("#layout_div");
        $('#loader').show();
        $.ajax({
            url: "../Dev/EbObjectList",
            type: "GET",
            cache: false,
            data: { "type": type44 },
            success: function (result) {
                _container.html(result);
                $('#loader').hide();              
            }
        });
    }
</script>