<div id="dbConnectionEdit" class="modal fade s-dash-modal" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <form action="" method="post" id="dbConnectionSubmit" data-toggle="validator">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">DataBase Connection</h4>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label>Database</label>
                                <select name="DatabaseVendor" class="form-control">
                                    <option value="PGSQL">PGSQL</option>
                                    <option value="MYSQL">MYSQL</option>
                                    <option value="MSSQL">MSSQL</option>
                                    <option value="ORACLE">ORACLE</option>
                                </select>
                            </div>
                            <div class="form-group">
                                <label>DataBase Name</label>
                                <input name="DatabaseName" type="text" class="form-control" required/>
                            </div>
                            <div class="form-group">
                                <label>Nick Name</label>
                                <input name="NickName" type="text" class="form-control" required/>
                            </div>
                        </div>
                        <div class="col-md-8">
                            <label>User Name</label>
                            <div class="col-md-12 pd-0">
                                <table class="table table-bordered table-form">
                                    <thead>
                                        <tr>
                                            <th style="width:130px;"></th>
                                            <th>User Name</th>
                                            <th>Password</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>Admin</td>
                                            <td><input name="UserName" type="text" class="form-control" required/></td>
                                            <td><input name="Password" type="text" class="form-control" required/></td>
                                        </tr>
                                        <tr>
                                            <td>Read-Write User</td>
                                            <td><input name="ReadWriteUserName" type="text" class="form-control" required/></td>
                                            <td><input name="ReadWritePassword" type="text" class="form-control" required/></td>
                                        </tr>
                                        <tr>
                                            <td>ReadOnly User</td>
                                            <td><input name="ReadOnlyUserName" type="text" class="form-control" required/></td>
                                            <td><input name="ReadOnlyPassword" type="text" class="form-control" required/></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label>Server Address</label>
                                <input name="Server" type="text" class="form-control" required/>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label>Port</label>
                                <input name="Port" pattern="^[_0-9]{1,}$" type="text" class="form-control" required/>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label>TimeOut</label>
                                <input name="Timeout" type="text" class="form-control" required/>
                            </div>
                        </div>
                        <input type="hidden" name="IsNew" />
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    <button type="submit" class="btn btn-primary">Save</button>
                </div>
            </form>
        </div>
    </div>
</div>
<div id="EmailConnectionEdit" class="modal fade" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
