    @if (IsSectionDefined("StyleSheet"))
    {
        @RenderSection("StyleSheet")
    }

    @Html.RenderStyles()

    @if (IsSectionDefined("JavaScript"))
    {
        @RenderSection("JavaScript")
    }

    @Html.RenderScripts()
    <script>
            AllMetasRoot = {};
            window.ebcontext = {
                sid: "@ViewBag.cid",
                user: @Html.Raw(ViewBag.UserObject) || null,
                menu: {},
                locations: {},
                header: {},
                env:'@ViewBag.Env',
                font:@Html.Raw(JsonConvert.SerializeObject(EbFont.FontFamilies)),
                EbObjectMeta:@Html.Raw(ViewBag.EbObjectTypeMeta),
                se_url: '@ViewBag.ServerEventUrl',
                setup: {},
                subscription_id: null
            };
    </script>
</head>
<body>
    <div class="html-root">
        @{
            if (ViewBag.wc == "dc" || ViewBag.wc == "uc")
            {
                <div id="eb-common-menu">
                    @{ await Html.RenderPartialAsync("EbQuickMenu");}
                </div>
            }
            if (ViewBag.wc == "uc")
            {
                <div id="eb-locswitch_wraper">
                    <div class="loc_switchModal_fade"></div>
                    <div class="loc_switchModal" id="loc_switchModal">
                        <div class="loc_switchModal_outer">
                            <div class="loc_switchModal_box">
                                <div class="locs_head">
                                    <h4>Switch Location</h4>
                                    <div class="loc_searchInput w-100">
                                        <input type="text" class="form-control" id="loc-search" placeholder="Search Location." />
                                    </div>
                                </div>
                                <div class="locs_bdy"></div>
                                <div class="locs_foot">
                                    <button class="btn loc-switchbtn pull-right" id="setLocSub">Switch</button>
                                </div>
                                <div class="no_loc_config">No Locations.</div>
                            </div>
                        </div>
                    </div>
                </div>
            }
        }
        <div id="layout_div" class="container-fluid html-root-layout">
            @RenderBody()
        </div>
        <div class="modal fade eb-common-popup" id="eb-common-popup" tabindex="-1" role="dialog" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title">Modal title</h5>
                        <button type="button" class="close" id="eb-common-popup-close" data-dismiss="modal" style="margin-left:auto;">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">

                    </div>
                    <div class="modal-footer">
                        <button type="button" id="eb-common-popup-ok" class="btn btn-primary">OK</button>
                    </div>
                </div>
            </div>
        </div>
        @{
            await Html.RenderPartialAsync("NotificationWindow");
        }
    </div>

    @if (ViewBag.wc == "uc" || ViewBag.wc == "dc")
    {
        <script>
            window.ebcontext.menu = new EbMenu({
                Console: '@ViewBag.wc',
                Sid: '@ViewBag.cid',
                Uid: '@ViewBag.UId'
            });
        </script>
    }

    @if (ViewBag.wc == "uc")
    {
        <script>
            let loc__ = new LocationPicker({
                Location: '@Html.Raw(ViewBag.Locations)',
                Tid: '@ViewBag.cid',
                Uid:'@ViewBag.UId',
                Current:@ViewBag.CurrentLocation
            });
            window.ebcontext.locations = loc__;
        </script>
    }

    @if (IsSectionDefined("JsCode"))
    {
        @RenderSection("JsCode")
    }
    @Html.RenderJsCode()

    <script>
        $(document).ready(function () {
            window.ebcontext.setup = new Setup({ se_url: '@ViewBag.ServerEventUrl' });
            $('.Eb_Image').Lazy();
        });
    </script>
</body>
</html>