
@{
    @using ExpressBase.Common;
    var dvobj = EbSerializers.Json_Serialize(ViewBag.dsObj);
}

<style>
    .tools {
        border: none;
        background: none;
        color: black;
    }
</style>
<link rel="stylesheet" type="text/css" href="~/css/splitWindow.css">
<script src="~/js/splitWindow.js" type="text/javascript"></script>

<div class="row">
    <div class="toolbar-com" id='Toolbar@(ViewBag.tabnum)'>
        <label class='dvname commonControls' style='color: #333;width:200px'></label>
        <button id='btnGo@(ViewBag.tabnum)'' class='tools commonControls' style='display:none'><i class="fa fa-play" aria-hidden="true"></i></button>
        <button id='Save_btn' class='tools commonControls' style="display:none">Save</button>
        <button id='fd_toggle' class='tools commonControls' style="display:none">Toggle</button>
    </div>
</div>
<div class="row dv" id="parent-div@(ViewBag.tabnum)" style="height:calc(100vh - 87px);">
</div>
<script>
    if (@ViewBag.tabnum === 0) {
        var commonO = $("#layout_div").data("commonObj");
    }
    var PcFlag = "False";

    commonO.ObjCollection["#vernav@(ViewBag.tabnum)"] = new EbDataTable('@ViewBag.Refid', '@ViewBag.VersionNumber', @ViewBag.ObjType, @Html.Raw(ViewBag.dsObj), '@ViewBag.Status', @ViewBag.tabnum, '@ViewBag.ServiceUrl');




</script>
