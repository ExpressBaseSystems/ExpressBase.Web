<table>
    <tr>
        <th colspan="2">Assembly Versions</th>
    </tr>


    @{
        try
        {
            foreach (string v in @ViewBag.Versions)
            {
                string[] p = v.Split("-");
                if (p != null && p.Length > 1)
                {
                    <tr>
                        <td>@p[0]</td>
                        <td>@p[1]</td>
                    </tr>
                }
            }
        }
        catch (Exception e)
        {
            Console.WriteLine(e.Message + e.StackTrace);
        }
    }
</table>
<style>
    table {
        border: 1px solid black;
        margin-left: 20%;
        margin-top: 5%;
    }

    th, td {
        border-bottom: 1px solid black;
    }
</style>