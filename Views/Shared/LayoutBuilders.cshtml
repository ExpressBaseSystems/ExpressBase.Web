<script>
    window.ebcontext.menu = new EbMenu({
        Console: '@ViewBag.wc',
        Sid: '@ViewBag.cid',
        Uid: '@ViewBag.UId'
    });
    $('.Eb_Image').Lazy();
</script>

@if (ViewBag.wc == "uc")
{
    <script>
        let loc__ = new LocSwitch({
            Location: '@Html.Raw(ViewBag.Locations)',
            Tid: '@ViewBag.cid',
            Uid:'@ViewBag.UId',
            Current:@ViewBag.CurrentLocation
        });
